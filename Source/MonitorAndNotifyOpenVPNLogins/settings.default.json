{
  "Application": {
    "OpenVpnLogFile": "/tmp/ovpn_status_2_result",
    "ReadOpenVpnLogFileEveryXSeconds": 30,
    "WhitelistVpnUsers": [ "yourvpnuser" ]
  },
  "PushOver": {
    "Enabled": false,
    "EndPoint": "https://api.pushover.net/1/messages.json",
    "ApiToken": "",
    "GroupOrUserKeys": [ "" ]
  },
  "Logging": {
    "MinimumLevel": "Verbose",
    "File": {
      "Enabled": true,
      "Path": "Logs/ApplicationLog.txt",
      "RollingInterval": "Day",
      "RollOnFileSizeLimit": true,
      "RestrictedToMinimumLevel": "Information"
    },
    "Console": {
      "Enabled": true,
      "RestrictedToMinimumLevel": "Information"
    }
  }
}